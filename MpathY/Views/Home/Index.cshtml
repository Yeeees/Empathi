@model MpathY.Models.wholeOrgList
@{
    ViewBag.Title = "Empathi";
    Layout = "~/Views/Shared/_mytemplate.cshtml";
}
<header>
    <style>
        nav ul {
            height: 200px;
            width: 100%;
        }

        nav ul {
            overflow: hidden;
            overflow-y: scroll;
        }

        .navbar-right li a {
            text-transform:capitalize;
            font-size:16px;
        }

            #sentences {
            display:block;
            height:500px;
            border: 1px solid black;
            line-height: 50px;
            list-style: none;
            padding: 0;
        }

        #sentences > li {
            font-size: 18px;
            text-align: center;
        }

        #sentences > li:hover {
            display: block;
            background-color: #028FCC;
            color: white;
        }

        .form-group input[type="radio"],
        .form-group input[type="checkbox"] {
            display: none;
            background-color: dimgrey;
        }

            .form-group input[type="checkbox"] + .btn-group > label,
            .form-group input[type="radio"] + .btn-group > label {
                white-space: normal;
            }

                .form-group input[type="checkbox"] + .btn-group > label.btn-default,
                .form-group input[type="radio"] + .btn-group > label.btn-default {
                    color: #333;
                    background-color: #fff;
                    border-color: #ccc;
                }

                .form-group input[type="checkbox"] + .btn-group > label.btn-primary,
                .form-group input[type="radio"] + .btn-group > label.btn-primary {
                    color: #fff;
                    background-color: #428bca;
                    border-color: #357ebd;
                }

                .form-group input[type="checkbox"] + .btn-group > label.btn-success,
                .form-group input[type="radio"] + .btn-group > label.btn-success {
                    color: #fff;
                    background-color: #5cb85c;
                    border-color: #4cae4c;
                }

                .form-group input[type="checkbox"] + .btn-group > label.btn-info,
                .form-group input[type="radio"] + .btn-group > label.btn-info {
                    color: #fff;
                    background-color: #5bc0de;
                    border-color: #46b8da;
                }

                .form-group input[type="checkbox"] + .btn-group > label.btn-warning,
                .form-group input[type="radio"] + .btn-group > label.btn-warning {
                    color: #fff;
                    background-color: #f0ad4e;
                    border-color: #eea236;
                }

                .form-group input[type="checkbox"] + .btn-group > label.btn-danger,
                .form-group input[type="radio"] + .btn-group > label.btn-danger {
                    color: #fff;
                    background-color: #d9534f;
                    border-color: #d43f3a;
                }

                .form-group input[type="checkbox"] + .btn-group > label.btn-link,
                .form-group input[type="radio"] + .btn-group > label.btn-link {
                    font-weight: normal;
                    color: #428bca;
                    border-radius: 0;
                }

                .form-group input[type="radio"] + .btn-group > label span:first-child,
                .form-group input[type="checkbox"] + .btn-group > label span:first-child {
                    display: none;
                }

                    .form-group input[type="radio"] + .btn-group > label span:first-child + span,
                    .form-group input[type="checkbox"] + .btn-group > label span:first-child + span {
                        display: inline-block;
                    }

            .form-group input[type="radio"]:checked + .btn-group > label span:first-child,
            .form-group input[type="checkbox"]:checked + .btn-group > label span:first-child {
                display: inline-block;
            }

                .form-group input[type="radio"]:checked + .btn-group > label span:first-child + span,
                .form-group input[type="checkbox"]:checked + .btn-group > label span:first-child + span {
                    display: none;
                }

            .form-group input[type="checkbox"] + .btn-group > label span[class*="fa-"],
            .form-group input[type="radio"] + .btn-group > label span[class*="fa-"] {
                width: 15px;
                float: left;
                margin: 4px 0 2px -2px;
            }

            .form-group input[type="checkbox"] + .btn-group > label span.content,
            .form-group input[type="radio"] + .btn-group > label span.content {
                margin-left: 10px;
                font-size: 18px;
            }

            .form-group input[type="checkbox"] + .btn-group > label:hover,
            .form-group input[type="radio"] + .btn-group > label:hover {
                margin-left: 10px;
                font-size: 18px;
                background-color: #028FCC;
                color: white;
            }

            .form-group input[type="checkbox"] + .btn-group:hover,
            .form-group input[type="radio"] + .btn-group:hover {
                margin-left: 10px;
                font-size: 18px;
                background-color: dimgrey;
                color: white;
            }

        #popup {
            display: none;
            position: relative;
            margin: 0 auto;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            box-shadow: 0px 0px 50px 2px #000;
            z-index:101;
        }

        .dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown button:hover {
            background-color: rgba(0,0,0,.2);
            color: white;
        }

        .navbar-right li button {
            color: #fff;
            font-size: 16px;
            font-weight: 600;
            padding-top: 30px;
            padding-bottom: 30px;
            background-color: #028FCC;
            border: none;
            position: relative;
            bottom: 4px;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            background-color: #f9f9f9;
            min-width: 160px;
            box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
            z-index: 99;
        }

        /* Links inside the dropdown */
        .dropdown-content a {
            color: black;
            padding: 12px 16px;
            text-decoration: none;
            display: block;
            background-color: white;
        }

        /* Change color of dropdown links on hover */
        .dropdown-content a:hover {
            background-color: grey;
            color: white;
        }

        /* Show the dropdown menu on hover */
        .dropdown:hover .dropdown-content {
            display: block;
        }

        .navbar-right li div div a {
            color: black;
            font-size: 14px;
            font-weight: 600;
            padding-top: 30px;
            padding-bottom: 30px;
        }

        .navbar-right li div div a:hover {
            color: white;
            background-color: #028FCC;
            font-size: 14px;
            font-weight: 600;
            padding-top: 30px;
            padding-bottom: 30px;
        }

        .popup-position {
            display:none;
            position:fixed;
            top:0;
            left:0;
            background-color: rgba(0,0,0,0.7);
            width:100%;
            height:100%;
            z-index:100;
        }


        #popup-wrapper{
            width:500px;
            margin: 70px auto;
            text-align: left;
        }
        #popup-container{

            background-color:white;
            padding:20px;
            border-radius:4px;
        }

        .fluid-element-wrapper {
            height: auto;
            padding-bottom: 0;
        }

        @@media (max-width: 1000px) {
            .content-to-hide a img {
                display: none;
            }

            .drop-this {
                position: relative;
                top: 500px;
            }

            @@media (max-width:799px) {
                .fluid-element-wrapper {
                    height: 60%;
                }

                .fluid-caption {
                    position: relative;
                    bottom: 40px;
                    right: 20px;
                }

                .fluid-caption1 {
                    position: relative;
                    bottom: 100px;
                    right: 20px;
                }

                .fluid-adjustment {
                    position: relative;
                    left: 5px;
                }

                .fluid-control {
                    position: relative;
                    bottom: 220px;
                }

                #map{
                    width:100%;
                }

                #conversation{
                    position:relative;
                    bottom:300px;
                }

                .drop-this {
                    position: relative;
                    top: 100px;
                }
            }
        }
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
</header>

