                            //two checkbox related
                            changeVoiceCheckbox = function (checkName) {
                                document.getElementById(checkName).checked = true;
                                var ul = document.getElementById("sentences");
                                var li = ul.getElementsByTagName('li');
                                var checkLabel;
                                switch (checkName) {
                                    case "Convo Humanitarian Settlement":
                                        checkLabel = "settlement";
                                        break;
                                    case "Convo drug and alcohol counselling":
                                        checkLabel = "counselling";
                                        break;
                                    case "Convo Volunteer":
                                        checkLabel = "volunteer";
                                        break;
                                    case "Convo Personal care and Food":
                                        checkLabel = "food";
                                        break;
                                    case "Convo general Legal service":
                                        checkLabel = "legal";
                                        break;
                                    case "Convo Accommodation":
                                        checkLabel = "accommodation";
                                        break;
                                    case "Convo Health":
                                        checkLabel = "health";
                                        break;
                                    case "Convo English education":
                                        checkLabel = "englesson";
                                        break;
                                    case "Convo Monetary and Job":
                                        checkLabel = "job";
                                        break;

                                }


                                // Loop through all list items, and hide those who don't match the search query
                                for (i = 0; i < li.length; i++) {
                                    var a = li[i].className;

                                    if (a.indexOf(checkLabel) !== -1 && document.getElementById(checkName).checked == true) {
                                        console.log("class name :" + a + "  " + checkLabel);
                                        li[i].style.display = "";
                                    } else {
                                        li[i].style.display = "none";
                                    }
